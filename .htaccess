ExpiresActive On
ExpiresDefault "access plus 12 hour"

<Files mailman.php>
	ExpiresDefault A0 
	Header set Cache-Control "private, no-cache, must-revalidate, max-age=0"
	Header set Pragma "no-cache"
</Files>
<Files ~ "\.php$">
	ExpiresDefault "access plus 12 hour"
</Files>
<Files index.php>
	ExpiresDefault "access plus 1 hour"
</Files>

Header append Cache-Control "public"
